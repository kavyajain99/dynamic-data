{{!-- .row class is from bootstraps grid --}}
<div class="row">
    {{!-- our image loop --}}
    <div class="row gallery">
        {{#each gallery.images}}
        <div class="col-3 galleryItem">
            <img class="img-fluid" src="{{this.url}}" alt="{{this.title}}">
        </div>
        {{/each}}
    </div>
</div>
{{!-- end row --}}